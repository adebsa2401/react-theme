{
  "name": "react-theme",
  "version": "1.0.0",
  "description": "Theme provider for React.js and React Native applications",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
